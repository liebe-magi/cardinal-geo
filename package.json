    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.55.0",
    "vite": "^5.2.0",
    "vite-plugin-pwa": "^1.2.0"
